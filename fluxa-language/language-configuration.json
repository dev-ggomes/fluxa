{
  "comments": {
    "blockComment": [
      "/*",
      "*/"
    ],
    "lineComment": {
      "comment": "#",
      "noIndent": false
    }
  },
  "brackets": [
    [
      "(",
      ")"
    ],
    [
      "[",
      "]"
    ],
    [
      "{",
      "}"
    ]
  ],
  "autoClosingPairs": [
    [
      "(",
      ")"
    ],
    [
      "[",
      "]"
    ],
    [
      "{",
      "}"
    ],
    [
      "'",
      "'"
    ],
    {
      "open": "\"", "close": "\""
    },
    [
      "/*",
      "*/"
    ]
  ],
  "surroundingPairs": [
    [
      "(",
      ")"
    ],
    [
      "[",
      "]"
    ],
    [
      "{",
      "}"
    ]
  ],
  "onEnterRules": [
    {
      "beforeText": ".*:\\s*$",
      "action": { "indent": "indent" }
    }
  ]
}